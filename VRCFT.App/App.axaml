<Application xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 x:Class="VRCFT.App.App"
			 xmlns:local="using:VRCFT.App"
			 xmlns:semi="https://irihi.tech/semi"
			 RequestedThemeVariant="Dark">

	<Application.DataTemplates>
		<local:ViewLocator />
	</Application.DataTemplates>

	<Application.Styles>
		<!--<FluentTheme />-->
		<semi:SemiTheme Locale="en-US" />
		<semi:AvaloniaEditSemiTheme />

		<Style Selector="Button">
			<Setter Property="IsEnabled" Value="{Binding Command, RelativeSource={RelativeSource Self}, Converter={x:Static ObjectConverters.IsNotNull}}" />
		</Style>
	</Application.Styles>

	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="/Assets/Themes/UiColor.axaml" />
				<!-- <ResourceInclude Source="/Assets/Themes/UiOverwrite.axaml" /> -->
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>

</Application>